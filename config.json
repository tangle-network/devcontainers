{
  "intermediate_templates": {
    "rust": "FROM base-system:latest\n\nENV RUSTUP_HOME=/usr/local/rustup \\\n    CARGO_HOME=/usr/local/cargo \\\n    PATH=/usr/local/cargo/bin:$PATH\n\nUSER root\nRUN curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y \\\n    && chmod -R a+w $RUSTUP_HOME $CARGO_HOME\n\nUSER project\n\nLABEL description=\"Rust intermediate layer\"\n"
  },
  "projects": {
    "coinbase": {
      "base": "base-system",
      "packages": {
        "npm": ["@coinbase/coinbase-sdk"]
      }
    },
    "injective": {
      "base": "base-system",
      "packages": {
        "npm": ["@injectivelabs/sdk-ts", "@injectivelabs/networks"]
      }
    },
    "succinct": {
      "base": "rust",
      "packages": {
        "cargo": ["sp1-sdk"]
      }
    },
    "risc0": {
      "base": "rust",
      "packages": {
        "cargo": ["risc0-zkvm"]
      }
    },
    "brevis": {
      "base": "rust",
      "packages": {
        "cargo": []
      }
    },
    "ethereum": {
      "base": "base-system",
      "packages": {
        "npm": ["ethers", "viem", "@wagmi/core"]
      }
    },
    "polygon": {
      "base": "base-system",
      "packages": {
        "npm": ["ethers", "viem", "@polygon-labs/sdk"]
      }
    },
    "zksync": {
      "base": "base-system",
      "packages": {
        "npm": ["zksync-ethers", "ethers"]
      }
    },
    "solana": {
      "base": "rust",
      "packages": {
        "cargo": ["solana-sdk", "anchor-lang"]
      }
    },
    "sui": {
      "base": "rust",
      "packages": {
        "cargo": ["sui-sdk"]
      }
    },
    "aptos": {
      "base": "rust",
      "packages": {
        "cargo": ["aptos-sdk"]
      }
    },
    "stylus": {
      "base": "rust",
      "packages": {
        "cargo": ["stylus-sdk"]
      }
    },
    "tangle": {
      "base": "rust",
      "packages": {
        "cargo": ["tangle-sdk"],
        "npm": ["@tangle-network/tangle-substrate-types"]
      }
    },
    "mongodb": {
      "base": "base-system",
      "packages": {
        "npm": ["mongodb"]
      }
    },
    "convex": {
      "base": "base-system",
      "packages": {
        "npm": ["convex"]
      }
    },
    "postgresql": {
      "base": "base-system",
      "packages": {
        "npm": ["pg", "@types/pg"]
      }
    },
    "rindexer": {
      "base": "rust",
      "packages": {
        "cargo": []
      }
    },
    "reth": {
      "base": "rust",
      "packages": {
        "cargo": ["reth"]
      }
    }
  }
}

